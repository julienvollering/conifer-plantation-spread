@article{buckleyDisturbanceInvasionReinvasion2007,
  title = {Disturbance, Invasion and Re-Invasion: Managing the Weed-Shaped Hole in Disturbed Ecosystems},
  shorttitle = {Disturbance, Invasion and Re-Invasion},
  author = {Buckley, Yvonne M. and Bolker, Benjamin M. and Rees, Mark},
  date = {2007},
  journaltitle = {Ecology Letters},
