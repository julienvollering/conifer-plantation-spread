@article{sadloSitespecificManagementInvasive2017,
  title = {Towards Site-Specific Management of Invasive Alien Trees Based on the Assessment of Their Impacts: The Case of {{Robinia}} Pseudoacacia},
  shorttitle = {Towards Site-Specific Management of Invasive Alien Trees Based on the Assessment of Their Impacts},
  author = {Sádlo, Jiří and Vítková, Michaela and Pergl, Jan and Pyšek, Petr},
