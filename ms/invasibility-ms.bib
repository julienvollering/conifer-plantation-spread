@article{hornTurbSeedModelWind2012,
  title = {{{TurbSeed}}—{{A}} Model for Wind Dispersal of Seeds in Turbulent Currents Based on Publicly Available Climate Data},
  author = {Horn, S. and Raabe, A. and Will, H. and Tackenberg, O.},
  date = {2012-07-10},
  journaltitle = {Ecological Modelling},
  shortjournal = {Ecological Modelling},
