@article{hewittTreeSeedDispersal2002,
  title = {Tree Seed Dispersal among Forest Fragments: {{II}}. {{Dispersal}} Abilities and Biogeographical Controls},
  shorttitle = {Tree Seed Dispersal among Forest Fragments},
  author = {Hewitt, Nina and Kellman, Martin},
  date = {2002},
  journaltitle = {Journal of Biogeography},
  volume = {29},
