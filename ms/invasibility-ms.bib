@article{reistadHighresolutionHindcastWind2011,
  title = {A High-Resolution Hindcast of Wind and Waves for the {{North Sea}}, the {{Norwegian Sea}}, and the {{Barents Sea}}},
  author = {Reistad, Magnar and Breivik, Øyvind and Haakenstad, Hilde and Aarnes, Ole Johan and Furevik, Birgitte R. and Bidlot, Jean-Raymond},
  date = {2011},
