@article{brusExtentDistributionOrigin2019,
  title = {Extent, Distribution and Origin of Non-Native Forest Tree Species in {{Europe}}},
  author = {Brus, Robert and Pötzelsberger, Elisabeth and Lapin, Katharina and Brundu, Giuseppe and Orazio, Christophe and Straigyte, Lina and Hasenauer, Hubert},
