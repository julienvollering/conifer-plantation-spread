}

@article{horvathDistributionModellingVegetation2019,
  ids = {horvathDistributionModellingVegetation},
  title = {Distribution Modelling of Vegetation Types Based on Area Frame Survey Data},
  author = {Horvath, Peter and Halvorsen, Rune and Stordal, Frode and Tallaksen, Lena Merete and Tang, Hui and Bryn, Anders},
